FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/hydrogenArea";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 5e5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    "(tankWall.*)"
    {
        type            zeroGradient;
    }

    "(front|back)"
    {
        type            wedge;
    }

    hydrogenArea_to_metalArea
    {
        type            fixedValue;
        value           $internalField;
    }
}


